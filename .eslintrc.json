{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": [2, "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": [
      "error",
      { "functions": true, "classes": true, "variables": false },
    ],
  },
}
